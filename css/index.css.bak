/* CSS Document*/
*{
	padding: 0px;
	margin: 0px;
	font-family: 微软雅黑;
}
body{
	width: 100%;
	/*height: 600px;*/
	background: url("../images/back.jpg") -0px -100px no-repeat;
}
.container div{
	display: none;
}
.container div:first-child{
	display: block;
}
.left p{
	font-size: 20px;
	font-weight: 700;
	color: #fff;
}
.userList .info img{
 	width: 50px;
 	height: 50px;
 	border-radius: 50%;
 	margin: 3px 0px;
 	position: relative;
 	left: 20px;
}
.content{
	width: 1014px;
	opacity: 0.3;
	background: #fff;
}
.content div{
	margin-right: 30px;
	display: none;
}
.waiting{
	margin-top: 60px;
	width: 1014px;
	height: 550px;
}
.waiting h2{
	text-align: center;
	line-height: 550px;
}
.chosing{
	width: 1014px;
	height: 550px;
	margin-top: 60px;
}
.chosing .role{
	width: 169px;
	height: 550px;
	background: #ccc;
	border-right: 1px solid red;
	float: left;
	margin:0px;
}
.playing{
	width: 1014px;
	height: 550px;
	background: #aff;
	margin-top: 60px;
}
/* create */
.ui-grid-a div{
	width: 40%;
	height: 70px;
	margin-bottom: 10px;
	text-align: center;
}
.ui-block-a {
	background-color: pink;
}
.ui-block-b {
	background-color: green;
}
#active {
	background-color: #ccc;
}
.div-createRoom {
	background-color: #ccc;
}
/*  */